  "version": "1.0.1",
  "description": "Personal Blog",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "client-install": "npm install --prefix client-blog",
    "backend-install": "npm install --prefix backend-blog",
